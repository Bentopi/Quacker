<div class="container">
  <div class="well">
    <%= form_tag do %>

      <div class="form-group">
        <%= label_tag :username %>
        <%= text_field_tag :username, params[:username], class: "form-control" %>
      </div>
      <div class="form-group">
        <%= label_tag :password %>
        <%= password_field_tag :password,"" ,class: "form-control" %>
      </div>
      <div class="form-group">
        <%= submit_tag "Sign In", class:'btn btn-primary btn-lg' %>
      </div>
      <% end %>
      <div class="">
        <h5> <%= link_to "Don't Have an Account? Sign up Here!", new_user_path %> </h5>
      </div>


  </div>
</div>
